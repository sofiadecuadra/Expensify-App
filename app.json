{
  "expo": {
    "name": "Expensify",
    "slug": "expensify",
    "scheme": "expensify",
    "description": "Expensify app for iOS and Android",
    "primaryColor": "#CB0C9F",
    "owner": "agustinferrari",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#CB0C9F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.agustinferrari.expensify"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#CB0C9F"
      },
      "package": "com.agustinferrari.expensify"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a1d9b294-e128-40c4-a9ff-dc6b66368f72"
      }
    }
  }
}
